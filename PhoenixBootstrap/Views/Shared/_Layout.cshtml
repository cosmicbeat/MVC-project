<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title - Phoenix Inspire</title>
    <!-- Bootstrap core CSS -->
    <link href="@Url.Content("~/css/bootstrap.css")" rel="stylesheet">
    <link href="@Url.Content("~/css/sticky-footer-navbar.css")" rel="stylesheet">

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>
    <!-- Fixed navbar -->
    <div class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">@ViewBag.Title</a>
            </div>
            <div class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <!--<li class="active"><a href="#">Home</a></li> -->
                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">About Us<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("History", "Index", "History")</li>
                            <li>@Html.ActionLink("Board", "Index", "Board")</li>
                            <li>@Html.ActionLink("Staff", "Index", "Staff")</li>
                            <li>@Html.ActionLink("CodeofConduct", "Index", "CodeofConduct")</li>
                            <li>@Html.ActionLink("AnnualReports", "Index", "AnnualReports")</li>
                            <!--<li class="divider"></li>
            <li class="dropdown-header">Nav header</li>
            <li><a href="#">Separated link</a></li>
            <li><a href="#">One more separated link</a></li> -->
                        </ul>
                    </li>
                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Our Work<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("FocusAreas", "Index", "FocusAreas")/a></li>
                            <li class="divider"></li>
                            <!--<li class="dropdown-header">Nav header</li> -->
                            <li>@Html.ActionLink("VocationalTraining", "Index", "VocationalTraining")/a></li>
                            <li>@Html.ActionLink("SocialEnterprise", "Index", "SocialEnterprise")</li>
                        </ul>
                    </li>
                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Our Partners<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Donors", "Index", "Donors")</li>
                            <li>@Html.ActionLink("Sponsors", "Index", "Sponsors")</li>
                            <li>@Html.ActionLink("Partnerships", "Index", "Partnerships")</li>
                        </ul>
                    </li>
                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Donate<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Cash", "Index", "Cash")></li>
                            <li>@Html.ActionLink("Stocks", "Index", "Stocks")</li>
                            <li>@Html.ActionLink("Bitcoins", "Index", "Bitcoins")</li>
                            <li>@Html.ActionLink("Otherassets", "Index", "Otherassets")></li>
                        </ul>
                    </li>
                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Blog<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Surveys", "Index", "Surveys")</li>
                        </ul>
                    </li>
                    <li>@Html.ActionLink("News", "Index", "News")<a href="#">Events</a></li>
                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Newsletters<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Subscriptionform", "Index", "Subscriptionform")</li>
                        </ul>
                    </li>
                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Volunteer<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Currentopportunities", "Index", "Currentopportunities")</li>
                            <li>@Html.ActionLink("Applyonline", "Index", "Applyonline")</li>
                        </ul>
                    </li>
                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Media Library<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Interview", "Index", "Interview")</li>
                            <li>@Html.ActionLink("News", "Index", "News")</li>
                            <!--<li><a href="#">Newsletters</a></li>-->
                        </ul>
                    </li>
                    <li><a href="#contact">Contact Us</a></li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div><!--/.nav-collapse -->
        </div>
    </div>

    @RenderBody()

    <div id="footer">
        <div class="container">
            <p class="text-muted">Place sticky footer content here.</p>
        </div>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="@Url.Content("~/js/bootstrap.js")"></script>
</body>
</html>